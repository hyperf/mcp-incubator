{
    "name": "hyperf/mcp",
    "description": "ModelContextProtocol for Hyperf",
    "keywords": [
        "php",
        "swoole",
        "hyperf",
        "mcp",
        "ModelContextProtocol"
    ],
    "require": {
        "php": ">=8.1",
        "hyperf/coordinator": "^3.1",
        "hyperf/di": "^3.1",
        "hyperf/http-server": "^3.1",
        "hyperf/rpc": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Hyperf\\Mcp\\": "src/"
        }
    },
    "extra": {
        "hyperf": {
            "config": "Hyperf\\Mcp\\ConfigProvider"
        }
    }
}
